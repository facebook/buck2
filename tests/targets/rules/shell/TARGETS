# @noautodeps

load("//buck2/tests/targets/rules:bzl_native.bzl", "bzl_native")

bzl_native.genrule(
    name = "copy_data",
    srcs = ["DATA"],
    bash = "cp $SRCS $OUT",
    out = "data.copy",
)

bzl_native.sh_binary(
    name = "diff",
    main = "diff.sh",
    resources = [
        "DATA",
        ":copy_data",
    ],
)

bzl_native.genrule(
    name = "gen",
    bash = "$(exe :diff) arg1 arg2 > $OUT",
    out = "out.txt",
)
