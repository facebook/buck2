load("@fbcode_macros//build_defs:native_rules.bzl", "buck_genrule")

buck_genrule(
    name = "long_build",
    out = "out.txt",
    bash = "python3 -c \"import time; time.sleep(5)\" ; echo HELLO, WORLD > $OUT",
    cmd_exe = "python3 -c \"import time; time.sleep(5)\"  ; echo HELLO, WORLD> %OUT%",
)

buck_genrule(
    name = "simple_build",
    out = "out.txt",
    bash = "echo HELLO, WORLD > $OUT",
    cmd_exe = "echo HELLO, WORLD> %OUT%",
)
