headers_dir(
    name = "headers",
    headers = ["app.h", "other.h"],
)

c_binary(
    name = "app",
    main = "app.c",
    headers = ["app.h", "other.h"],
    headers_dir_written = "$(@query_outputs :headers)",
    unused_command_line_param = read_config("test", "unused_command_line_param", "default"),
)

c_binary(
    name = "app_with_dummy_config",
    main = "app.c",
    headers = ["app.h", "other.h"],
    headers_dir_written = "$(@query_outputs :headers)",
    unused_command_line_param = read_config("test", "unused_command_line_param", "default"),
    _ignored = read_config("test", "dummy_config"),
)

simple_dep_file(
    name = "simple_dep_file",
    used_input1_contents = read_config("test", "used_input1_contents", "default"),
    used_input2_contents = read_config("test", "used_input2_contents", "default"),
    unused_input1_contents = read_config("test", "unused_input1_contents", "default"),
    unused_input2_contents = read_config("test", "unused_input2_contents", "default"),
)

shared_dir_dep_file(
    name = "shared_dir_dep_file",
    used_input_contents = read_config("test", "used_input_contents", "default"),
    unused_input_contents = read_config("test", "unused_input_contents", "default"),
)
