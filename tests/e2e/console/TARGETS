    name = "test_console",
    srcs = ["test_console.py"],
    env = {
        "FIXTURES": "$(location :fixtures)",
    },
)

buck_filegroup(
    name = "fixtures",
    srcs = glob(
        ["fixtures/**"],
    ),
)
