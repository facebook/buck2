# @noautodeps

load("@fbcode_macros//build_defs:python_binary.bzl", "python_binary")
load("@fbcode_macros//build_defs:python_library.bzl", "python_library")

oncall("buck2")

python_binary(
    name = "bin",
    srcs = [
        "bin.py",
    ],
    base_module = "",
    main_module = "bin",
    deps = [
        ":lib",
    ],
) if not read_config("python", "package_style") == "standalone" else None

python_library(
    name = "lib",
    srcs = [
        "bin.py",
    ],
    base_module = "",
)
