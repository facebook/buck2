# @noautodeps

load("@fbcode_macros//build_defs:cpp_binary.bzl", "cpp_binary")
load("//buck2/tests/targets/rules:bzl_native.bzl", "bzl_native")

bzl_native.cxx_library(
    name = "top",
    srcs = ["bar/bar.cpp"],
    include_directories = ["baz"],
    raw_headers = ["baz/bar.h"],
    exported_deps = ["fbcode//buck2/tests/targets/rules/cxx/raw_headers/foo:bottom"],
)

cpp_binary(
    name = "my_binary",
    srcs = ["main.cpp"],
    deps = [":top"],
)

bzl_native.cxx_binary(
    name = "bin",
    srcs = [
        "bar.cpp",
    ],
    include_directories = [
        "",
    ],
    raw_headers = [
        "foo.h",
    ],
    deps = [
        "fbsource//xplat/third-party/linker_lib:c",
    ],
)

