load("//rules:rules.bzl", "foo_library")

# Rules that share some sources (common.txt) and have unique sources
# Used to test intersect/except on FileSet results from inputs()
foo_library(
    name = "lib_a",
    srcs = ["lib_a.txt", "common.txt"],
    visibility = ["PUBLIC"],
)

foo_library(
    name = "lib_b",
    srcs = ["lib_b.txt", "common.txt"],
    visibility = ["PUBLIC"],
)

foo_library(
    name = "app",
    deps = [":lib_a", ":lib_b"],
    visibility = ["PUBLIC"],
)
