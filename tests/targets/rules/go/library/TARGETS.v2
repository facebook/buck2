buck_genrule(
    name = "check",
    cmd = "$(exe :hello_world) | grep 'hello world' > $OUT",
    out = "out.txt",
)

go_binary(
    name = "hello_world",
    srcs = [
        "main.go",
    ],
    deps = [
        ":lib",
    ],
)

go_library(
    name = "lib",
    srcs = [
        "lib.go",
    ],
)
